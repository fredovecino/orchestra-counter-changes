<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Counter</title>
    <link rel="stylesheet" href="css/main.css" />
</head>

<body>
    <div class="qm-app">
        <!-- Qmatic Header -->
        <header class="qm-header">

        </header>


        <!-- Qmatic Main -->
        <main class="qm-main">

            <!-- Qmatic Main Left -->
            <div class="qm-main__left">
                <div class="qm-card-layout">
                    <div class="qm-card">
                        <header class="qm-card__header">
                            Hello
                        </header>
                        <div class="qm-card__content">

                        </div>
                        <footer class="qm-card__footer">

                        </footer>
                    </div>
                </div>

                <!-- Qmatic Action Bar -->
                <div class="qm-action-bar">

                </div>
            </div>


            <!-- Qmatic Main Right -->
            <div class="qm-main__right">

         
                {% import "partials/button.nunjucks" as button  %}
                {{button.type('qm-button--secondary')}}
            
                <!-- Qmatic Action Buttons -->
                <button class="qm-button qm-button--primary">
                    <span class="qm-button__text" title="Primary">Primary</span>
                </button>
                <button class="qm-button qm-button--secondary">
                    <span class="qm-button__text" title="Secondary">Secondary</span>
                </button>
                
                <!-- Qmatic Action Buttons Full -->
                <button class="qm-button qm-button--primary qm-button--with-icon qm-button--full">
                    <i class="qm-button__icon" aria-hidden="true"></i>
                    <span class="qm-button__text" title="Primary full (icon)">Primary full (icon)</span>
                </button>
                <button class="qm-button qm-button--secondary qm-button--with-icon qm-button--full">
                    <i class="qm-button__icon" aria-hidden="true"></i>
                    <span class="qm-button__text" title="Secondary full (icon)">Secondary full (icon)</span>
                </button>
                <button class="qm-button qm-button--primary qm-button--full">
                    <span class="qm-button__text" title="Primary full">Primary full</span>
                </button>
                <button class="qm-button qm-button--secondary qm-button--full">
                    <span class="qm-button__text" title="Secondary full">Secondary full</span>
                </button>

                <!-- Qmatic Action Buttons Disabled -->
                <button class="qm-button qm-button--primary" disabled>
                    <span class="qm-button__text" title="Primary">Primary disabled</span>
                </button>
                <button class="qm-button qm-button--secondary" disabled>
                    <span class="qm-button__text" title="Secondary">Secondary disabled</span>
                </button>
            </div>
        </main>
        <footer class="qm-footer">
            <div class="qm-footer__content">
                <p class="qm-footer__text">Powered by Qmatic</p>
            </div>
        </footer>
    </div>
</body>

</html>