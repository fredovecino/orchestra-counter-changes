        <div class="qm-card-scroll-container__content">
            <div class="qm-card {% block cardModifier %}{% endblock %}">
                {% block header %}{% endblock %}
    
                {% block content %}{% endblock %}
    
                {% block footer %}{% endblock %}
            </div>
        </div> 
    </div>
</div>